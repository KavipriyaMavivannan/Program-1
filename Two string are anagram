package com.kavipriya;

import java.util.Scanner;

public class Main {

    public static void main( String[] args) {
    Scanner in = new Scanner(System.in);
    String d=in.nextInt();
    char a[]=d.toCharArray();
     int count=0;
       for(char c:a){
         switch (c){
           case 'a':
           case 'e':
           case 'i':
           case 'o':
           case 'u':
             count++;
              break;
       }
     }
   System.out.println(count);
  }
}
